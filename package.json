{
  "name": "cloudflare_apollo_server",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "lint": "run-p -l -c lint:*",
    "lint:eslint": "eslint './**/*.{js,ts,tsx}' --format codeframe --color --max-warnings=0",
    "lint:type": "tsc --pretty",
    "lint:format": "prettier -c './**/*.{js,ts,tsx}'",
    "build:cloudflare": "pnpm lint && node ./scripts/build/cloudflare.js",
    "format": "prettier -w './**/*.{js,ts,tsx}'",
    "dev": "wrangler dev",
    "deploy:dev": "wrangler publish",
    "deploy:prod": "NODE_ENV=production wrangler publish --env prod"
  },
  "author": "",
  "license": "MIT",
  "engines": {
    "node": "14",
    "pnpm": ">= 6.5.0"
  },
  "dependencies": {
    "apollo-server-cloudflare": "^3.3.0",
    "buffer": "^6.0.3",
    "graphql": "^15.6.0"
  },
  "devDependencies": {
    "@cfworker/dev": "^1.9.0",
    "@cloudflare/workers-types": "^2.2.2",
    "@cloudflare/wrangler": "^1.19.3",
    "@typescript-eslint/eslint-plugin": "^4.31.2",
    "@typescript-eslint/parser": "^4.31.2",
    "esbuild": "^0.13.1",
    "esbuild-plugin-alias": "^0.1.2",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.4.1",
    "typescript": "^4.4.3"
  }
}
